{
  "name": "multi-language-test",
  "version": "0.0.0",
"scripts": {
  "build": "ng build --localize",
  "postbuild": "npm run copy-locale-folders",
  "copy-locale-folders": "rm -rf netlify-dist && mkdir -p netlify-dist/en netlify-dist/ar-AE && cp -r dist/your-app-name/browser/en/* netlify-dist/en/ && cp -r dist/your-app-name/browser/ar-AE/* netlify-dist/ar-AE/ && cp _redirects netlify-dist/"
},

  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.3",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/localize": "^20.1.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "@netlify/angular-runtime": "^2.4.0"
  }
}
